Algoritmo "consumo"

Var

   distancia : inteiro
   combustivel, consumo : real

Inicio

      escreva("Distancia percorrida: ")
      leia(distancia)
      escreva("Combustivel gasto: ")
      leia(combustivel)

      consumo <- distancia / combustivel

      escreva("Consumo Medio = ", consumo:5:3)

Fimalgoritmo