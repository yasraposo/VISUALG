Algoritmo "teste_para"

Var

   i, N, cont, soma : inteiro

Inicio

   escreva("Quantos numeros serao digitados? ")
   leia(N)
   
   soma <- 0
   
   para i de 1 ate N faca
   
      escreva("Digite um numero: ")
         leia(cont)
         
   soma <- soma + cont
   fimpara

   escreval("SOMA = ", soma)

Fimalgoritmo