Algoritmo "maior"

Var

   num1, num2 : real
   
Inicio
   escreval("Digite um numero: ")
   leia(num1)
   escreval("Digite outro numero: ")
   leia(num2)
   Se (num1 < num2) Entao
      escreva("O segundo numero é o maior!")
      Senao
        escreval("O numero 1 é:", num1)
        escreval("O numero 2 é:", num2)
   FimSe


Fimalgoritmo